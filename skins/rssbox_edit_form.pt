<metal:html xmlns:tal="http://xml.zope.org/namespaces/tal" 
            xmlns:metal="http://xml.zope.org/namespaces/metal" 
            use-macro="here/main_template/macros/master">

<metal:main fill-slot="main">

<metal:boxform use-macro="here/basebox_edit_form/macros/boxform">

<metal:fields fill-slot="additional_fields">
  <tr valign="top">
    <th align="right">Channel</th>
    <td>
      <select name="channel_id">
        <option tal:repeat="aFeed here/portal_rss/objectValues" value="" tal:attributes="value aFeed/id" tal:content="aFeed/title">Feed Title</option>
      </select>
    </td>
  </tr>
</metal:fields>

</metal:boxform>

</metal:main>

</metal:html>
